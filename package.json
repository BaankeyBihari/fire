{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint -- --fix",
    "post-update": "yarn upgrade --latest",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "dockerize": "docker build -t fire.next.server .",
    "docker:compose-down": "docker-compose down",
    "docker:compose-up": "docker-compose up -d",
    "docker:clean": "docker system prune --volumes -f",
    "docker:down": "npm run docker:compose-down && npm run docker:clean",
    "docker:up": "npm run docker:down && npm run dockerize && npm run docker:compose-up"
  },
  "dependencies": {
    "@date-io/date-fns": "2.14.0",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/lab": "5.0.0-alpha.80",
    "@mui/material": "latest",
    "@mui/styles": "5.6.2",
    "@mui/system": "5.6.4",
    "@mui/x-date-pickers": "5.0.0-alpha.2",
    "chart.js": "3.7.1",
    "date-fns": "2.28.0",
    "formik": "2.2.9",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-chartjs-2": "4.1.0",
    "react-dom": "18.1.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.18.0",
    "@types/node": "17.0.24",
    "@types/react": "18.0.8",
    "@types/yup": "0.29.14",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "eslint": "8.14.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.30.0",
    "prettier": "2.6.2",
    "typescript": "4.6.4"
  }
}
