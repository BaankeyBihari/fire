{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "post-update": "yarn upgrade --latest",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "dockerize": "docker build -t fire.next.server .",
    "docker:compose-down": "docker-compose down",
    "docker:compose-up": "docker-compose up -d",
    "docker:clean": "docker system prune --volumes -f",
    "docker:down": "yarn docker:compose-down && yarn docker:clean",
    "docker:up": "yarn docker:down && yarn dockerize && yarn docker:compose-up",
    "commit": "git-cz",
    "cz": "git-cz",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "pre-commit": "yarn typecheck && yarn lint && yarn test --passWithNoTests --testPathIgnorePatterns=useFileOperations.test.ts",
    "validate": "yarn typecheck && yarn lint && yarn test --passWithNoTests --testPathIgnorePatterns=useFileOperations.test.ts"
  },
  "dependencies": {
    "@date-io/date-fns": "3.2.1",
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.2",
    "@mui/lab": "7.0.0-beta.17",
    "@mui/material": "7.3.2",
    "@mui/system": "7.3.2",
    "@mui/x-date-pickers": "8.11.1",
    "@tanstack/react-table": "^8.21.3",
    "chart.js": "4.5.0",
    "date-fns": "4.1.0",
    "formik": "2.4.6",
    "namor": "3.0.1",
    "next": "15.5.3",
    "react": "19.1.1",
    "react-chartjs-2": "5.3.0",
    "react-dom": "19.1.1",
    "yup": "1.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "24.3.1",
    "@types/react": "19.1.13",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "9.35.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react": "7.37.5",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-junit": "^16.0.0",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "5.9.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
